import java.util.*;
class Str_empty
{

    public static void main(String args[]) {
        String str = "";
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the string");
        str = sc.nextLine();
        if(str.isEmpty())
        {
            System.out.println("The given is  empty");
        }
        else
        {
            System.out.println("The given is  not empty");
        }
    }

}
