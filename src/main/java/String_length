import java.util.*;
class Str_length
{
    public static void main(String args[]) {
        String str = "";
        int count=0;
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the string");
        str = sc.nextLine();
        char[] c=str.toCharArray();
        for(char i:c)
        {
            count++;
        }
        System.out.println("The length of the string is "+count);
    }

}

